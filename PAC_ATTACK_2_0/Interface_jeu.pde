//Rendu Pac-Man
void render() {
  for ( int i=-1; i < 2; i++) {
    for ( int j=-1; j < 2; j++) {
      pushMatrix();
      translate(playerX + (i * width), playerY + (j*height));
      if ( direction == -1) { 
        rotate(PI);}
      arc(0, 0, 30, 30, map((millis() % 500), 0, 500, 0, 0.52), map((millis() % 500), 0, 500, TWO_PI, 5.76) );
      popMatrix();
     }
  }
}
